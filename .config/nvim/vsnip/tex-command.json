{
  "package": {
    "prefix": "use",
    "body": ["\\usepackage{$1}"]
  },
  "section": {
    "prefix": "sec",
    "body": ["\\section{$1}"]
  },
  "subsection": {
    "prefix": "ssec",
    "body": ["\\subsection{$1}"]
  },
  "boldface": {
    "prefix": "bf",
    "body": ["\\textbf{$1}"]
  },
  "emph": {
    "prefix": "em",
    "body": ["\\emph{$1}"]
  },
  "item": {
    "prefix": "i",
    "body": ["\\item"]
  },
  "bibliogaphy": {
    "prefix": "bib",
    "body": [
      "\\bibliographystyle{chicago}",
      "\\bibliography{${1:\\jobname}}"
    ]
  }
}
