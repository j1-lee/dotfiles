{
  "environment": {
    "prefix": "env",
    "body": [
      "\\begin{$1}",
      "\t$0",
      "\\end{$1}"
    ]
  },
  "align": {
    "prefix": "ali",
    "body": [
      "\\begin{align*}",
      "\t$0",
      "\\end{align*}"
    ]
  },
  "displaymath": {
    "prefix": "dis",
    "body": [
      "\\begin{displaymath}",
      "\t$0",
      "\\end{displaymath}"
    ]
  },
  "gather": {
    "prefix": "gat",
    "body": [
      "\\begin{gather*}",
      "\t$0",
      "\\end{gather*}"
    ]
  },
  "itemize": {
    "prefix": "ite",
    "body": [
      "\\begin{itemize}",
      "\t\\item $0",
      "\\end{itemize}"
    ]
  },
  "enumerate": {
    "prefix": "enu",
    "body": [
      "\\begin{enumerate}",
      "\t\\item $0",
      "\\end{enumerate}"
    ]
  },
  "center": {
    "prefix": "cen",
    "body": [
      "\\begin{center}",
      "\t$0",
      "\\end{center}"
    ]
  },
  "figure": {
    "prefix": "fig",
    "body": [
      "\\begin{figure}[ht!]",
      "\t\\centering",
      "\t\\includegraphics[width=0.8\\textwidth]{$1}",
      "\t\\caption{$2}",
      "\\end{figure}"
    ]
  }
}
